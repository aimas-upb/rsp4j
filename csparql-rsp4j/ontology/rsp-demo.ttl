@prefix : <http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix consert-core: <http://pervasive.semanticweb.org/ont/2017/07/consert/core#> .
@base <http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo> .

<http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo> rdf:type owl:Ontology ;
                                                              owl:imports <http://pervasive.semanticweb.org/ont/2017/07/consert/core> ;
                                                              rdfs:comment "Ontology file containing instances of ContextEntities and ContextAssertions in the context-aware access to smart devices scenario. The scenario proposes using RSP to enable context-aware web access control."@en ;
                                                              rdfs:label "RSP Demo Lab 308"@en ;
                                                              owl:versionInfo "1.0.0" .

#################################################################
#    Classes
#################################################################

###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#AvailabilityStatus
:AvailabilityStatus rdf:type owl:Class ;
                    rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#HasAvailabilityStatus
:HasAvailabilityStatus rdf:type owl:Class ;
                       rdfs:subClassOf consert-core:BinaryContextAssertion ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionObject ;
                                         owl:allValuesFrom :AvailabilityStatus
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionSubject ;
                                         owl:allValuesFrom :Device
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionAcquisitionType ;
                                         owl:hasValue consert-core:Sensed
                                       ] .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#LocatedAt
:LocatedAt rdf:type owl:Class ;
           rdfs:subClassOf consert-core:BinaryContextAssertion ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionAcquisitionType ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( consert-core:Profiled
                                                             consert-core:Sensed
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionObject ;
                             owl:allValuesFrom :Room
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionSubject ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Device
                                                               :Person
                                                             )
                                               ]
                           ] .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#OwnedBy
:OwnedBy rdf:type owl:Class ;
         rdfs:subClassOf consert-core:BinaryContextAssertion ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionObject ;
                           owl:allValuesFrom :Organization
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionSubject ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( :Device
                                                             :Room
                                                           )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionAcquisitionType ;
                           owl:hasValue consert-core:Profiled
                         ] .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#Room
:Room rdf:type owl:Class ;
      rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#WorksAt
:WorksAt rdf:type owl:Class ;
         rdfs:subClassOf consert-core:BinaryContextAssertion ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionObject ;
                           owl:allValuesFrom :Organization
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionSubject ;
                           owl:allValuesFrom :Person
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionAcquisitionType ;
                           owl:hasValue consert-core:Profiled
                         ] .


#################################################################
#    Individuals
#################################################################

###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#alex
:alex rdf:type owl:NamedIndividual ,
               :Person .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#alexWorkPlace
:alexWorkPlace rdf:type owl:NamedIndividual ,
                        :WorksAt ;
               consert-core:assertionObject :upb ;
               consert-core:assertionSubject :alex .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#available
:available rdf:type owl:NamedIndividual ,
                    :AvailabilityStatus .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#blinds308Location
:blinds308Location rdf:type owl:NamedIndividual ,
                            :LocatedAt ;
                   consert-core:assertionAcquisitionType consert-core:Profiled ;
                   consert-core:assertionObject :lab308 ;
                   consert-core:assertionSubject :blindsLab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#blinds308ownership
:blinds308ownership rdf:type owl:NamedIndividual ,
                             :OwnedBy ;
                    consert-core:assertionObject :upb ;
                    consert-core:assertionSubject :blindsLab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#blindsLab308
:blindsLab308 rdf:type owl:NamedIndividual ,
                       :Device .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#hueLamp308Location
:hueLamp308Location rdf:type owl:NamedIndividual ,
                             :LocatedAt ;
                    consert-core:assertionAcquisitionType consert-core:Profiled ;
                    consert-core:assertionObject :lab308 ;
                    consert-core:assertionSubject :hueLampLab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#hueLamp308ownership
:hueLamp308ownership rdf:type owl:NamedIndividual ,
                              :OwnedBy ;
                     consert-core:assertionObject :upb ;
                     consert-core:assertionSubject :hueLampLab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#hueLampLab308
:hueLampLab308 rdf:type owl:NamedIndividual ,
                        :Device .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#lab308
:lab308 rdf:type owl:NamedIndividual ,
                 :Room .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#lab308ownership
:lab308ownership rdf:type owl:NamedIndividual ,
                          :OwnedBy ;
                 consert-core:assertionObject :upb ;
                 consert-core:assertionSubject :lab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#notAvailable
:notAvailable rdf:type owl:NamedIndividual ,
                       :AvailabilityStatus .


###  http://aimas.cs.pub.ro/consert/ontologies/2021/09/rsp-demo#upb
:upb rdf:type owl:NamedIndividual ,
              :Organization .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AvailabilityStatus
                :Device
                :Organization
                :Person
                :Room
              )
] .


