@prefix : <http://aimas.cs.pub.ro/consert/ontologies/precis#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix consert: <http://pervasive.semanticweb.org/ont/2017/07/consert/core#> .

:alex rdf:type owl:NamedIndividual ,
               foaf:Person .

:upb rdf:type owl:NamedIndividual ,
              :Organization .

:lab308 rdf:type owl:NamedIndividual ,
                 :Room .

:blindsLab308 rdf:type owl:NamedIndividual ,
                       :Device .

:hueLampLab308 rdf:type owl:NamedIndividual ,
                        :Device .

###  http://aimas.cs.pub.ro/consert/ontologies/precis#alexLocation
:alexLocation rdf:type owl:NamedIndividual ,
                             :LocatedAt ;
                    consert:assertionAcquisitionType consert:Sensed ;
                    consert:assertionObject :lab308 ;
                    consert:assertionSubject :alex .