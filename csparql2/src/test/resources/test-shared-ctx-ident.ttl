@prefix : <http://aimas.cs.pub.ro/consert/ontologies/precis#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix consert-ann: <http://pervasive.semanticweb.org/ont/2017/07/consert/annotation#> .
@prefix consert-core: <http://pervasive.semanticweb.org/ont/2017/07/consert/core#> .
@base <http://aimas.cs.pub.ro/consert/ontologies/precis> .

<http://aimas.cs.pub.ro/consert/ontologies/precis> rdf:type owl:Ontology ;
                                                    owl:imports <http://pervasive.semanticweb.org/ont/2017/07/consert/annotation> ,
                                                                <http://pervasive.semanticweb.org/ont/2017/07/consert/core> ;
                                                    rdfs:comment "Ontology file containing instances of ContextEntities and ContextAssertions in the context-aware access to smart devices scenario. The scenario proposes using RSP to enable context-aware web access control."@en ;
                                                    rdfs:label "RSP Demo Lab 308"@en ;
                                                    owl:versionInfo "1.0.0" .

#################################################################
#    Classes
#################################################################

###  http://aimas.cs.pub.ro/consert/ontologies/precis#AvailabilityStatus
:AvailabilityStatus rdf:type owl:Class ;
                    rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#HasAvailabilityStatus
:HasAvailabilityStatus rdf:type owl:Class ;
                       rdfs:subClassOf consert-core:BinaryContextAssertion ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionObject ;
                                         owl:allValuesFrom :AvailabilityStatus
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionSubject ;
                                         owl:allValuesFrom :Device
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty consert-core:assertionAcquisitionType ;
                                         owl:hasValue consert-core:Sensed
                                       ] .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#LocatedAt
:LocatedAt rdf:type owl:Class ;
           rdfs:subClassOf consert-core:BinaryContextAssertion ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionAcquisitionType ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:oneOf ( consert-core:Profiled
                                                             consert-core:Sensed
                                                           )
                                               ]
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionObject ;
                             owl:allValuesFrom :Room
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty consert-core:assertionSubject ;
                             owl:allValuesFrom [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Device
                                                               :Person
                                                             )
                                               ]
                           ] .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#OwnedBy
:OwnedBy rdf:type owl:Class ;
         rdfs:subClassOf consert-core:BinaryContextAssertion ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionObject ;
                           owl:allValuesFrom :Organization
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionSubject ;
                           owl:allValuesFrom [ rdf:type owl:Class ;
                                               owl:unionOf ( :Device
                                                             :Room
                                                           )
                                             ]
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionAcquisitionType ;
                           owl:hasValue consert-core:Profiled
                         ] .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#Room
:Room rdf:type owl:Class ;
      rdfs:subClassOf consert-core:ContextEntity .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#WorksAt
:WorksAt rdf:type owl:Class ;
         rdfs:subClassOf consert-core:BinaryContextAssertion ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionObject ;
                           owl:allValuesFrom :Organization
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionSubject ;
                           owl:allValuesFrom :Person
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty consert-core:assertionAcquisitionType ;
                           owl:hasValue consert-core:Profiled
                         ] .


#################################################################
#    Individuals
#################################################################

###  http://aimas.cs.pub.ro/consert/ontologies/precis#alex
:alex rdf:type owl:NamedIndividual ,
               :Person .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#alexWorkPlace
:alexWorkPlace rdf:type owl:NamedIndividual ,
                        :WorksAt ;
               consert-ann:hasAnnotation :alexWorkTemporalAnn ;
               consert-core:assertionObject :upb ;
               consert-core:assertionSubject :alex .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#alexWorkTemporalAnn
:alexWorkTemporalAnn rdf:type owl:NamedIndividual ,
                              consert-ann:TemporalValidityAnnotation ;
                     consert-ann:endTime "2060-02-01T10:00:00"^^xsd:dateTime ;
                     consert-ann:startTime "2020-02-01T10:00:00"^^xsd:dateTime .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#available
:available rdf:type owl:NamedIndividual ,
                    :AvailabilityStatus .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#blindsLab308
:blindsLab308 rdf:type owl:NamedIndividual ,
                       :Device .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#hueLampLab308
:hueLampLab308 rdf:type owl:NamedIndividual ,
                        :Device .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#lab308
:lab308 rdf:type owl:NamedIndividual ,
                 :Room .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#lab308ownership
:lab308ownership rdf:type owl:NamedIndividual ,
                          :OwnedBy ;
                 consert-core:assertionObject :upb ;
                 consert-core:assertionSubject :lab308 .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#notAvailable
:notAvailable rdf:type owl:NamedIndividual ,
                       :AvailabilityStatus .


###  http://aimas.cs.pub.ro/consert/ontologies/precis#upb
:upb rdf:type owl:NamedIndividual ,
              :Organization .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AvailabilityStatus
                :Device
                :Organization
                :Person
                :Room
              )
] .


